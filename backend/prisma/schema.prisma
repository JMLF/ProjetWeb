// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement())
  name    String?
  surname String?
  civility  Civility   @relation(fields: [civilityId], references: [id])
  civilityId Int @default(1)
}

model Civility {
  id Int @id @default(autoincrement())
  status String 
  user User[]
}

